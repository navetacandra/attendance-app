                  "value": {
                    "mode": "add"
                  }
                },
                "presence": {
                  "value": {
                    "mode": "presence"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful update mode",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Response"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "data": {
