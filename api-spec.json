              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Response"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "data": {
                          "$ref": "#/components/schemas/Mode"
                        }
                      }
                    }
                  ]
                },
                "examples": {
                  "add": {
                    "value": {
                      "success": true,
                      "code": 200,
                      "data": {
                        "mode": "add"
                      }
                    }
                  },
                  "presence": {
                    "value": {
                      "success": true,
                      "code": 200,
                      "data": {
                        "mode": "presence"
                      }
                    }
                  }
                }
              },
